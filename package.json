{
  "name": "node-server",
  "version": "1.0.0",
  "main": "src/app.ts",
  "scripts": {
    "start": "pm2 start ecosystem.config.json",
    "stop": "pm2 stop all",
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/app.ts",
    "compile": "npx tsc",
    "build": "npm-run-all compile && tsc-alias",
    "swagger": "ts-node src/swagger.ts"
  },
  "dependencies": {
    "@types/express": "5.0.0",
    "express": "4.21.2",
    "npm-run-all": "4.1.5",
    "rimraf": "6.0.1",
    "swagger-autogen": "2.23.7",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0"
  },
  "author": "seunghee.choi",
  "license": "ISC",
  "description": "api server boilerplate",
  "devDependencies": {
    "@types/node": "22.13.5",
    "@types/swagger-jsdoc": "6.0.4",
    "@types/swagger-ui-express": "4.1.8",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.1",
    "tsc-alias": "1.8.11",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.7.3"
  }
}
