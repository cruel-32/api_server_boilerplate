{
  "name": "node-server",
  "script": "./build/app.js",
  "cwd": ".",
  "watch": true,
  "ignore_watch": [
    "node_modules",
    "logs"
  ],
  "exec_mode": "cluster",
  "instances": "3",
  "env": {
    "NODE_ENV": "production"
  },
  "output": "./logs/pm2/console.log",
  "error": "./logs/pm2/consoleError.log"
}